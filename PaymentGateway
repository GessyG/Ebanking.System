        this.informacioneGateway = informacioneGateway;
        this.celesiPublik = celesiPublik;
        this.celesiPrivat = celesiPrivat;
    }

    public boolean verifikoPagesën(double shuma, String informacioneKarteKrediti) {
        // Logjika për verifikimin e pagesës
        // Kthe një boolean që tregon nëse pagesa është verifikuar ose jo
        return true;
    }

    public boolean autorizoTransaksionin(String informacioneKarteKrediti, double shuma, String destinacioni) {
        // Logjika për autorizimin e transaksionit
        // Kthe një boolean që tregon nëse transaksioni është autorizuar ose jo
        return true;
    }

    public void ruajTëDhënatEKartaveKrediti(String informacioneKarteKrediti) {
        // Logjika për ruajtjen e të dhënave të kartave të kreditit
    }
}
